<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zzmhome.mybatisplus.mapper.UserMapper">
    <select id="selectMapById" resultType="map">
        select id,name,age,email
        from user
        where id = #{id}
    </select>
    
    <select id="selectPageBuyAge" resultType="User">
        select user_id,user_name, user_age, email from t_user where user_age > #{age}
    </select>

</mapper>